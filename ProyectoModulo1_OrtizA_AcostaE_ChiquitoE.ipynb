    "def plet(time=0):\n",
    "    plt.figure(figsize=(6,5))\n",
    "    x=xx[:,0]\n",
    "    y=xx[:,2]\n",
    "    z=time\n",
    "    plt.scatter(xx[:,0], xx[:,2], c= xx[:,0] , s = 8, lw = 0)\n",
    "    #plt.plot(z, x[int(time*(1/salto2))], 'ko', ms =10, label='Planeta 1')\n",
