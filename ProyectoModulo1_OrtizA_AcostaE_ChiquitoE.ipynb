    "    #plt.plot(z, y[int(time*(1/salto2))], 'mD', ms =10, label='Planeta 2')\n",
    "    plt.plot(x[int(time*(1/salto2))], y[int(time*(1/salto2))], 'mD', ms =10, label='Planeta 2')\n",
